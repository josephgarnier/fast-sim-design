{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Debugger",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "${defaultBuildTask}",
      "program": "",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "avoidWindowsConsoleRedirection": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for lldb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "exceptions": true,
        "moduleLoad": true,
        "programOutput": true,
        "engineLogging": false,
        "trace": false,
        "traceResponse": false
      },
      "visualizerFile": "${workspaceFolder}/.vscode/qt6.natvis.xml",
      "showDisplayString": true,
      "additionalSOLibSearchPath": "",
      "symbolLoadInfo": {
        "loadAll": true,
        "exceptionList": ""
      },
      "linux": {
        "program": "${workspaceFolder}/bin/fast-sim-design",
        "additionalSOLibSearchPath": "/opt/Qt/6.8.0/gcc_64/bin"
      },
      "windows": {},
      "osx": {}
    },
    {
      "name": "(gdb) Debugger",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "${defaultBuildTask}",
      "program": "",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "avoidWindowsConsoleRedirection": false,
      "MIMode": "gdb",
      "miDebuggerPath": "",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "exceptions": true,
        "moduleLoad": true,
        "programOutput": true,
        "engineLogging": false,
        "trace": false,
