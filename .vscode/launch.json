{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) Debugger",
			"type": "cppdbg",
			"request": "launch",
			"preLaunchTask": "${defaultBuildTask}",
			"program": "${workspaceFolder}/bin/fast-sim-design",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"avoidWindowsConsoleRedirection": true,
			"MIMode": "lldb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for lldb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"logging": {
				"exceptions": true,
				"moduleLoad": false,
				"programOutput": true,
				"engineLogging": false,
				"trace": false,
				"traceResponse": false
			},
			"visualizerFile": "${workspaceFolder}/.vscode/default.natvis",
			"showDisplayString": false
		},
		{
			"name": "(gdb) Debugger",
			"type": "cppdbg",
			"request": "launch",
			"preLaunchTask": "${defaultBuildTask}",
			"program": "${workspaceFolder}/bin/fast-sim-design",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"avoidWindowsConsoleRedirection": true,
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/local/bin/gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"logging": {
				"exceptions": true,
				"moduleLoad": false,
				"programOutput": true,
				"engineLogging": false,
				"trace": false,
				"traceResponse": false
			},
			"visualizerFile": "${workspaceFolder}/.vscode/default.natvis",
			"showDisplayString": false
		}
	]
}